[Unit]
Description=Youtrack
Requires=network.target
After=syslog.target network.target

[Service]
Type=simple
WorkingDirectory=/opt/youtrack
ExecStart=/usr/bin/java -jar {{youtrack_path_jar}} --J-Xmx1024m --J-Djava.security.egd=/dev/./urandom --data-dir={{youtrack_data_path}} --no-browser 8080
User=youtrack

[Install]
WantedBy=default.target